{
  "name": "audioplayer",
  "description": "An electron audioplayer by ramgendeploy",
  "author": {
    "name": "Ramiro",
    "url": "https://ramagg.com"
  },
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "app": "electron .",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "homepage": ".",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "13.6.6",
    "electron-is-dev": "^1.2.0",
    "concurrently": "^5.2.0",
    "electron-builder": "^22.7.0",
    "wait-on": "^5.0.0"
  },
  "build": {
    "appId": "audioplayer.ramgen",
    "mac": {
      "category": "audioplayer.app.music.audio"
    }
  }
}
